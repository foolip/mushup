<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.musicbrainz.hibernate">

	<class name="HibernateArtistAlias" table="artistalias">
		<comment>MusicBrainz Artist Alias.</comment>

		<id name="row" column="id">
			<generator class="sequence">
				<param name="sequence">artistalias_id_seq</param>
			</generator>
		</id>

		<property name="name" type="string" column="name" />
	</class>

</hibernate-mapping>
